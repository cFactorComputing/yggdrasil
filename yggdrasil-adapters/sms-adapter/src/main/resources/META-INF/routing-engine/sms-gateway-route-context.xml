<?xml version="1.0" encoding="UTF-8"?>
<beans:beans xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://camel.apache.org/schema/spring"
             xmlns:context="http://www.springframework.org/schema/context"
             xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
       http://camel.apache.org/schema/spring http://camel.apache.org/schema/spring/camel-spring.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">


    <routeContext id="io.swiftwallet.yggdrasil.adapters.sms">
        <route id="io.swiftwallet.yggdrasil.adapters.sms.receive">
            <from uri="direct://SMS_ADAPTER"/>
            <process ref="smsUriProcessor"/>
            <to uri="https4://127.0.0.1/"/>
        </route>
    </routeContext>
</beans:beans>